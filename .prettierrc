{
  "printWidth": 120,
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": false,
  "bracketSameLine": false,
  "useTabs": true,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "proseWrap": "always",
  "overrides": [
    {
      "files": [
        "*.less"
      ],
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ]
}
